                  "id": "62af8ccf-ff78-41aa-86bb-639c37cdde1f",
                  "type": "scriptCondition",
                  "name": "Is Nearby",
                  "script": [
                    "return loc.leader:GetPosition():Distance(self:GetPosition()) < 40"
                  ]
                },
                {
                  "id": "1e41ce92-b612-462a-8393-e9461133115b",
                  "type": "scriptCondition",
                  "name": "Is Idle",
                  "script": [
                    "return loc.isIdle"
                  ]
                }
              ],
              "target": [
                "behaviors_borecky_jan",
