  "subtrees": [
    {
      "name": "Find Friend",
      "id": "1dbc76b2-0d0c-4ec2-96ae-521d12f1bd21",
      "type": "scriptAction",
      "script": [
        "for i, entity in ipairs(",
        "    GetEntitiesInArea(",
        "        Circle(",
        "            self:GetPosition(), ",
        "            100",
        "        )",
        "    )",
        ") ",
        "do",
        "    loc.leader = entity",
        "    loc.lastLeaderPosition = entity:GetPosition()",
        "    break",
        "end"
      ]
    },
    {
      "name": "<unnamed>",
      "id": "b786049e-473f-4cba-b8e3-2649fb1c563e",
      "type": "supervisedParallel",
      "subtrees": [
        {
